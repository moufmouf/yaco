{
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:container-interop/definition-interop.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:mnapoli/assembly.git"
        }
    ],
    "name": "thecodingmachine/yaco",
    "type": "library",
    "description": "YACO (Yet Another COmpiler) is a PHP tool that generates a PHP container based on entry definitions.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TheCodingMachine\\Yaco\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodingMachine\\Yaco\\": "tests/"
        }
    },
    "require": {
        "container-interop/definition-interop": "dev-master",
        "container-interop/container-interop": "^1.0",
        "jeremeamia/superclosure": "^2.0",
        "mouf/picotainer": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mnapoli/assembly": "dev-master",
        "satooshi/php-coveralls": "dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
